# build Packages

if [ "${CBF['os_id']}" = 'alpine' ]; then
    declare PKGS='alpine-sdk build-base curl expat-dev file findutils g++ gcc gcc-objc git gnutls-utils jpeg libc6-compat libtool libxml2-dev linux-headers make mysql perl-plack rsync'
    package.checkIfInstalled "$PKGS"
    apk add --no-cache --virtual .buildDepedencies $PKGS 
fi